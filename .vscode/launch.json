{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Generate CSharp beta",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Typewriter/bin/Debug/net5.0/Typewriter.dll",
      "args": [
        "-v",
        "Info",
        "-m",
        "${workspaceFolder}/../msgraph-metadata/clean_beta_metadata/cleanMetadataWithDescriptionsbeta.xml",
        "-g",
        "Files",
        "-l",
        "CSharp",
        "-o",
        "${workspaceFolder}/../OutputCSharpbeta",
        "-e",
        "beta"
      ],
      "cwd": "${workspaceFolder}/src/Typewriter/bin/Debug/net5.0",
      "console": "internalConsole",
      "stopAtEntry": false
    },
