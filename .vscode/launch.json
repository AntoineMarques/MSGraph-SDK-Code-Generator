{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": ".NET Core Launch (console)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceFolder}/src/Typewriter/bin/Debug/net5.0/Typewriter.dll",
			"args": [
                "-v",
                "Info",
                "-m",
                "C:/sources/github/msgraph-metadata/clean_v10_metadata/cleanMetadataWithDescriptionsv1.0.xml",
                "-g",
                "Files",
                "-l",
                "Java",
                "-o",
                "C:/sources/github/msgraph-sdk-java/src/main/java",
                "-e",
                "v1.0"
            ],
			"cwd": "${workspaceFolder}/src/Typewriter",
			"console": "internalConsole",
			"stopAtEntry": false
		},
		{
			"name": ".NET Core Attach",
			"type": "coreclr",
			"request": "attach",
			"processId": "${command:pickProcess}"
		}
	]
}